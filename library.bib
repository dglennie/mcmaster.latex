file = {:C$\backslash$:/Users/Diana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Madsen, Patterson, Wilson - 1992 - The use of India ink as an optical absorber in tissue-simulating phantoms.pdf:pdf},
