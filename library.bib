file = {:C$\backslash$:/Users/Diana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gade et al. - 2006 - Diffuse reflectance spectrophotometry with visible light comparison of four different methods in a tissue phantom.pdf:pdf},
issn = {0031-9155},
journal = {Physics in medicine and biology},
keywords = {Algorithms,Animals,Hemoglobins,Hemoglobins: chemistry,Humans,Image Interpretation, Computer-Assisted,Light,Models, Chemical,Mucous Membrane,Mucous Membrane: pathology,Oximetry,Oximetry: methods,Oxygen,Oxygen: chemistry,Phantoms, Imaging,Scattering, Radiation,Spectrophotometry,Spectrophotometry: methods,Spectroscopy, Near-Infrared,Spectroscopy, Near-Infrared: methods,Swine,Temperature,Tomography, Optical},
