journal = {Journal of applied physiology (Bethesda, Md. : 1985)},
keywords = {Adult,Artifacts,Blood Flow Velocity,Female,Heat Stress Disorders,Heat Stress Disorders: metabolism,Humans,Male,Muscle, Skeletal,Muscle, Skeletal: blood supply,Muscle, Skeletal: metabolism,Oximetry,Oximetry: methods,Oxygen,Oxygen: analysis,Oxygen: metabolism,Reproducibility of Results,Sensitivity and Specificity,Skin,Skin: blood supply,Spectrophotometry, Infrared,Spectrophotometry, Infrared: methods},
