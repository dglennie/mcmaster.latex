language = {english},
number = {3},
pages = {131--136},
publisher = {Karger Publishers},
title = {{Quantitative Reflection Spectrophotometry: Spatial and Temporal Variation of Hb Oxygenation in Human Skin}},
url = {http://www.karger.com/Article/FullText/178965},
volume = {15},
year = {1995}
}
