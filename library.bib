keywords = {Animals,Mice,Nude,Oximetry,Oximetry: methods,Oxygen Consumption,Perfusion Imaging,Perfusion Imaging: methods,Radiodermatitis,Radiodermatitis: diagnosis,Radiodermatitis: physiopathology,Reproducibility of Results,Sensitivity and Specificity},
