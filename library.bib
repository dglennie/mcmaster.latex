journal = {Applied optics},
keywords = {Epidermis,Epidermis: anatomy \& histology,Humans,Melanins,Melanins: analysis,Models, Biological,Monte Carlo Method,Optical Processes,Oxygen,Oxygen: blood,Scattering, Radiation,Skin,Skin: anatomy \& histology,Skin: blood supply,Skin: chemistry,Spectrophotometry,Spectrophotometry: methods,Spectrophotometry: statistics \& numerical data},
month = apr,
number = {10},
pages = {1707--19},
pmid = {20357850},
title = {{Rapid and accurate estimation of blood saturation, melanin content, and epidermis thickness from spectral diffuse reflectance.}},
