issn = {2156-7085},
journal = {Biomedical Optics Express},
month = sep,
number = {10},
pages = {2381--2394},
title = {{\v{C}erenkov radiation emission and excited luminescence (CREL) sensitivity during external beam radiation therapy: Monte Carlo and tissue oxygenation phantom studies}},
