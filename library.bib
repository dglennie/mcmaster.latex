file = {:C$\backslash$:/Users/Diana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Vano-Galvan et al. - 2013 - Dynamic skin changes of acute radiation dermatitis revealed by in vivo reflectance confocal microscopy.pdf:pdf},
issn = {1468-3083},
journal = {Journal of the European Academy of Dermatology and Venereology : JEADV},
keywords = {Acute Disease,Aged,Aged, 80 and over,Breast Neoplasms,Breast Neoplasms: radiotherapy,Female,Humans,Microscopy, Confocal,Middle Aged,Prospective Studies,Radiodermatitis,Radiodermatitis: pathology,Skin,Skin: pathology},
month = sep,
