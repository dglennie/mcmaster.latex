issn = {0196-8092},
journal = {Lasers in surgery and medicine},
keywords = {Forearm,Forearm: blood supply,Humans,Infrared Rays,Infrared Rays: diagnostic use,Laser Therapy,Skin,Skin: blood supply,Varicose Veins,Varicose Veins: diagnosis,Veins},
month = jan,
